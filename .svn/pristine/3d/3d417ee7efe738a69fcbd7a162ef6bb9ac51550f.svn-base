#!/bin/sh
wget=/usr/bin/wget
CURRENT_DIR= $pwd
SCRIPTS_DIR="$(pwd)/Scripts"
cd $SCRIPTS_DIR
python getRequests.py $1 $2
python topAsn.py
python getUrls.py $1 $2

while IFS='\n' read path;do
    echo $path
    $wget $path
    filename=$(basename $path .bz2)
    ext=".bz2"
    bzip2 -d $filename$ext
    python parseUpdate.py $filename

done < urlFile
rm -rf update*
python write_to_csv.py $1
